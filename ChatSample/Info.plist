	<string>Konverz needs access to Audio when you want to share it to your friends</string>
	<key>NSCameraUsageDescription</key>
	<string>Konverz needs access to Camera to make Video calls</string>
	<key>NSContactsUsageDescription</key>
	<string>ChatSample will continuously upload your contacts to its encrypted cloud server to let you connect with your friends across all your devices.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Konverz needs access to Location when you want to share it to your friends</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>Konverz needs access to Location when you want to share it to your friends</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Konverz needs access to Location when you want to share it to your friends</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Konverz needs access to Microphone to make calls</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Konverz needs access to Photos when you want to share to friends</string>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Registration</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>UIStatusBarStyle</key>
	<string>UIStatusBarStyleLightContent</string>
